<!DOCTYPE html>
<html>
<head>
  <title>MentorMapp</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
  <%= render 'layouts/shim' %>
</head>

<body>


  <%= render 'layouts/header' %>
  <div class="container">
    <% if notice %><h1 class="notice"><%= notice %></h1><% end %>
    <% if alert %><h1 class="alert"><%= alert %></h1><% end %>
  </div>

<%= yield %>


  <%= render 'layouts/footer' %>
  
</body>
</html>